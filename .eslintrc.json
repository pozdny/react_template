{
    "env": {
        "browser": true
    },
    "extends": "eslint:recommended",
    "parserOptions": {
        "ecmaFeatures": {
            /*"experimentalObjectRestSpread": true,*/
            "jsx": true
        }
    },
    "plugins": [
        "react"
    ],
    "rules": {
        "global-require": 0,
        "quotes": [
            "error",
            "single"
        ],
        "semi": [
            "error",
            "always"
        ],
        "no-undef": 0,
        "no-console": 1,
        "no-unused-vars": 1
    }
}